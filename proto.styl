  animation-iteration-count: infinite;
  animation-timing-function: ease-out;
}
@keyframes pendulum {
  0% {
    transform: translateX(-50px);
  }
  50% {
    transform: translateX(50px);
  }
  100% {
    transform: translateX(-50px);
  }
}
.swing:after {
  content: '';
  box-sizing: border-box;
  display: inline-block;
  height: 20px;
  width : 20px;
