* {
  box-sizing: border-box;
}
.is--loading {
  position: relative;
  height: 200px;
}
.duo:before {
  content: "";
  display: inline-block;
  height: 40px;
  width: 40px;
  border-radius: 100%;
  background: linear-gradient(to bottom, red 20px, blue 20px);
  position: absolute;
  top: 50%;
  margin-top: -20px;
  left: 50%;
  margin-left: -20px;
  box-shadow: 0px 4px 10px black;
}
.duo:after {
  content: "";
  display: inline-block;
  height: 20px;
  width: 20px;
  border-radius: 100%;
  border: 5px solid black;
  border-top-color: red;
  border-right-color: red;
  border-bottom-color: blue;
  border-left-color: blue;
  position: absolute;
  top: 50%;
  margin-top: -15px;
  left: 50%;
  transform: rotate(-45deg);
  margin-left: -15px;
  animation-duration: .75s;
  animation-name: spin;
  animation-iteration-count: infinite;
}
@keyframes spin {
  from {
    transform: rotate(-45deg);
  }
  to {
    transform: rotate(315deg);
  }
}
.whirl:after {
  content: "";
  display: inline-block;
  height: 20px;
  width: 20px;
  border-radius: 100%;
  background-color: red;
  position: absolute;
  top: 50%;
  margin-top: -10px;
  left: 50%;
  margin-left: -10px;
  animation-duration: 2s;
  animation-name: whirl;
  animation-iteration-count: infinite;
  animation-timing-function: linear;
}
@keyframes whirl {
  0% {
    transform: scale(0) rotate(-45deg) translateY(0px);
    background-color: green;
  }
  50% {
    transform: scale(1.25) rotate(960deg) translateY(30px);
    background-color: blue;
  }
  100% {
    transform: scale(0) rotate(2000deg) translateY(0px);
    background-color: green;
  }
}
.travel:after {
  content: "";
  display: inline-block;
  height: 20px;
  width: 20px;
  border-radius: 100%;
  background-color: orange;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -10px;
  margin-top: -10px;
  transform-origin: left;
  animation-duration: 1.5s;
  animation-name: travel;
  animation-iteration-count: infinite;
  /* animation-timing-function: linear; */
}
@keyframes travel {
  0% {
    transform: translateY(-20px) translateX(-20px);
    background-color: red;
  }
  25% {
    transform: translateX(-20px) translateY(20px);
    background-color: green;
  }
  50% {
    transform: translateY(20px) translateX(20px);
    background-color: yellow;
  }
  75% {
    transform: translateX(20px) translateY(-20px);
    background-color: blue;
  }
  100% {
    transform: translateY(-20px) translateX(-20px);
    background-color: red;
  }
}
.bloom:before {
  content: '';
  display: inline-block;
  height: 40px;
  width : 40px;
  background-color: blue;
  border-radius: 100%;
  left: 50%;
  top: 50%;
  position: absolute;
  margin-left: -20px;
  margin-top: -20px;
}
.bloom:after {
  content: '';
  box-sizing: border-box;
  display: inline-block;
  height: 60px;
  width : 60px;
  border: 10px solid white;
  border-radius: 100%;
  left: 50%;
  top: 50%;
  position: absolute;
  margin-left: -30px;
  margin-top: -30px;
  animation-name: bloom;
  animation-duration: 1s;
  animation-iteration-count: infinite;
  animation-timing-function: ease-out;
}
@keyframes bloom {
  0%{
    transform: scale(0);
  }
  100% {
    transform: scale(1);
  }
}

.bounce:before {
  content: '';
  display: inline-block;
  height: 40px;
  width : 40px;
  background-color: blue;
  border-radius: 100%;
  left: 50%;
  top: 50%;
  position: absolute;
  margin-left: -20px;
  margin-top: -20px;
  animation-name: bounce;
  animation-duration: 1.5s;
  animation-iteration-count: infinite;
  /* animation-direction: alternate; */
  animation-delay: .75s;
  opacity: .5;
}
.bounce:after {
  content: '';
  box-sizing: border-box;
  display: inline-block;
  height: 40px;
  width : 40px;
  background-color: red;
  opacity: .5;
  border-radius: 100%;
  left: 50%;
  top: 50%;
  position: absolute;
  margin-left: -20px;
  margin-top: -20px;
  animation-name: bounce;
  animation-duration: 1.5s;
  animation-iteration-count: infinite;
}
@keyframes bounce {
  0% {
    transform: scale(0);
  }
  50% {
    transform: scale(1);
  }
  100% {
    transform: scale(0);
  }
}
.flyby:after {
  content: '';
  box-sizing: border-box;
  display: inline-block;
  height: 20px;
  width : 20px;
  background-color: green;
  border-radius: 100%;
  left: 50%;
  top: 50%;
  position: absolute;
  margin-left: -10px;
  margin-top: -10px;
  transform: scale(0);
  animation-name: flyby;
  animation-duration: 1s;
  animation-iteration-count: infinite;
}
@keyframes flyby {
  0% {
    opacity: 0;
    background-color: red;
    transform: scale(0) translateX(-50px);
  }
  50% {
    opacity: 1;
    background-color: green;
    transform: scale(1.5) translateX(0px);
  }
  100% {
    background-color:red;
    opacity: 0;
    transform: scale(0) translateX(50px);
  }
}
.fadeby:after {
  content: '';
  box-sizing: border-box;
  display: inline-block;
  height: 20px;
  width : 20px;
  background-color: green;
  border-radius: 100%;
  left: 50%;
  top: 50%;
  position: absolute;
  margin-left: -10px;
  margin-top: -10px;
  animation-name: fadeby;
  animation-duration: 2.5s;
  animation-iteration-count: infinite;
}
@keyframes fadeby {
  0% {
    background: green;
    opacity: 0;
    transform: translateX(-20px);
  }
  10% {
    opacity: 1;
    transform: translateX(-20px);
  }
  20% {
    opacity: 0;
    transform: translateX(-20px);
  }
  30% {
    transform: translateX(-20px);
    opacity: 0;
  }
  40% {
    opacity: 0;
    transform: translateX(0px);

  }
  50% {
    background: purple;
    opacity: 1;
    transform: translateX(0px);

  }
  60% {
    opacity: 0;
    transform: translateX(0px);

  }
  70% {
    opacity: 0;
    transform: translateX(20px);
  }
  80% {
    background: blue;
    opacity: 1;
    transform: translateX(20px);
  }
  90% {
    opacity: 0;
    transform: translateX(20px);

  }
  100% {
    opacity: 0;
    transform: translateX(0px);
  }
}
.wheel:after {
  content: '';
  box-sizing: border-box;
  display: inline-block;
  height: 30px;
  width : 30px;
  border: solid 15px black;
  border-right-color: red;
  border-left-color: red;
  border-radius: 100%;
  left: 50%;
  top: 50%;
  position: absolute;
  margin-left: -15px;
  margin-top: -15px;
  animation-name: wheel;
  animation-duration: .5s;
  animation-iteration-count: infinite;
  animation-timing-function: linear;
}
@keyframes wheel {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}
.pendulum:after {
  content: '';
  box-sizing: border-box;
  display: inline-block;
  height: 20px;
  width : 20px;
  background-color: red;
  border-radius: 100%;
  left: 50%;
  top: 50%;
