.cyclical,
.merge {
  animation-name: spin;
  animation-duration: 2s;
  animation-iteration-count: infinite;
  animation-timing-function: ease-out;
}
.cyclical:before {
  content: '';
  display: inline-block;
  height: 20px;
  width : 20px;
  background-color: blue;
  border-radius: 100%;
  left: 50%;
  top: 50%;
  position: absolute;
  transform-origin: -60% -80%;
  margin-left: -10px;
  margin-top: -10px;
